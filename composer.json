{
    "name": "magento/project-community-edition",
    "description": "eCommerce Platform for Growth (Community Edition)",
    "type": "project",
    "license": [
        "OSL-3.0",
        "AFL-3.0"
    ],
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "magento/magento-composer-installer": true,
            "magento/inventory-composer-installer": true,
            "laminas/laminas-dependency-plugin": true,
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "magento/composer-root-update-plugin": true,
            "magento/composer-dependency-version-audit-plugin": true
        }
    },
    "version": "2.4.2-p1",
    "require": {
        "ampersand/magento2-disable-stock-reservation": "^1.2",
        "aune-io/magento2-product-grid-category-filter": "^1.3",
        "dominicwatts/customerlogin": "^1.0",
        "facebook/php-business-sdk": "^12.0",
        "laminas/laminas-serializer": "^2.13",
        "lessclick-system/barcode": "^3.0",
        "magento/composer-root-update-plugin": "~2.0",
        "magento/product-community-edition": "2.4.8",
        "mageplaza/magento-2-arabic-language-pack": "dev-master",
        "mageplaza/magento-2-seo-extension": "^4.2",
        "mageplaza/magento-2-social-login": "^4.1",
        "mageplaza/module-ajax-layered-navigation": "^4.0",
        "mageplaza/module-better-popup": "^4.1",
        "mageplaza/module-delete-orders": "^4.0",
        "mageplaza/module-instagram-feed": "^4.0",
        "mageplaza/module-sitemap": "^4.2",
        "mageplaza/module-smtp": "^4.7",
        "magepow/categories": "^1.0",
        "magepow/layerednav": "^1.0",
        "magepow/speedoptimizer": "^1.0",
        "magiccart/magicmenu": "^1.0",
        "stripe/stripe-php": "^7.86",
        "tecnickcom/tcpdf": "^6.4",
        "tnw/module-stripe": "^2.3"
    },
    "autoload": {
        "exclude-from-classmap": [
            "**/dev/**",
            "**/update/**",
            "**/Test/**"
        ],
        "files": [
            "app/etc/NonComposerComponentRegistration.php"
        ],
        "psr-0": {
            "": [
                "app/code/",
                "generated/code/"
            ]
        },
        "psr-4": {
            "Magento\\": "app/code/Magento/",
            "Magento\\Framework\\": "lib/internal/Magento/Framework/",
            "Magento\\Setup\\": "setup/src/Magento/Setup/",
            "Zend\\Mvc\\Controller\\": "setup/src/Zend/Mvc/Controller/"
        }
    },
    "require-dev": {
        "dealerdirect/phpcodesniffer-composer-installer": "^0.7.0",
        "magento/magento-coding-standard": "*",
        "phpcompatibility/php-compatibility": "^9.3",
        "phpmd/phpmd": "^2.8.0",
        "phpunit/phpunit": "^9",
        "sebastian/phpcpd": "^6.0.3",
        "squizlabs/php_codesniffer": "~3.5.4"
    },
    "conflict": {
        "gene/bluefoot": "*"
    },
    "autoload-dev": {
        "psr-4": {
            "Magento\\PhpStan\\": "dev/tests/static/framework/Magento/PhpStan/",
            "Magento\\Sniffs\\": "dev/tests/static/framework/Magento/Sniffs/",
            "Magento\\TestFramework\\Inspection\\": "dev/tests/static/framework/Magento/TestFramework/Inspection/",
            "Magento\\TestFramework\\Utility\\": "dev/tests/static/framework/Magento/TestFramework/Utility/",
            "Magento\\Tools\\": "dev/tools/Magento/Tools/",
            "Magento\\Tools\\Sanity\\": "dev/build/publication/sanity/Magento/Tools/Sanity/"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "repositories": {
        "0": {
            "type": "composer",
            "url": "https://repo.magento.com/"
        }
    },
    "extra": {
        "magento-force": "override"
    }
}
