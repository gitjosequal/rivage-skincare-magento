# Magento 2 .gitignore

# Generated files
/generated/*
!/generated/.htaccess

# Cache files
/var/cache/*
!/var/cache/.htaccess

# Session files
/var/session/*
!/var/session/.htaccess

# Log files
/var/log/*
!/var/log/.htaccess

# Temporary files
/var/tmp/*
!/var/tmp/.htaccess

# Page cache
/var/page_cache/*
!/var/page_cache/.htaccess

# Import/Export files
/var/import/*
!/var/import/.htaccess

# Backup files
/var/backups/*
!/var/backups/.htaccess

# Static files
/pub/static/*
!/pub/static/.htaccess

# Media files (uncomment if you don't want to track media)
# /pub/media/*
# !/pub/media/.htaccess

# Vendor directory (managed by Composer)
/vendor/

